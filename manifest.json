{
"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

"manifest_version": 2,
    "name": "[PGHMusicScene4Truth]",
    "description": "[A chrome extension to keep it real in the Pittsburgh music scene]",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]


}
